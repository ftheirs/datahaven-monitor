{
	"$schema": "https://biomejs.dev/schemas/2.3.8/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"style": {
				"recommended": false
			},
			"suspicious": {
				"noConsole": "off"
			}
		}
	},
	"files": {
		"ignoreUnknown": false,
		"includes": ["**", "!!**/dist"]
	},
	"assist": {
		"enabled": false
	}
}
